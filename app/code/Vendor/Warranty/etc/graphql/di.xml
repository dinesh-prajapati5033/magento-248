        <arguments>
            <argument name="repository" xsi:type="object">Vendor\Warranty\Api\WarrantyRegistrationRepositoryInterface</argument>
            <argument name="identity" xsi:type="object">Vendor\Warranty\Model\GraphQL\Resolver\WarrantyRegistrationIdentity</argument>
